{
  "containerDefinitions": [
    {
      "name": "weelo-backend",
      "image": "318774499084.dkr.ecr.ap-south-1.amazonaws.com/weelo-backend:latest",
      "cpu": 0,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp",
          "name": "weelo-port",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "JWT_REFRESH_SECRET",
          "value": "__SET_IN_ECS__"
        },
        {
          "name": "AWS_LOCATION_ENABLED",
          "value": "true"
        },
        {
          "name": "AWS_REGION",
          "value": "ap-south-1"
        },
        {
          "name": "MSG91_AUTH_KEY",
          "value": ""
        },
        {
          "name": "PORT",
          "value": "3000"
        },
        {
          "name": "AWS_LOCATION_ROUTE_CALCULATOR",
          "value": "weelo-routes"
        },
        {
          "name": "MSG91_SENDER_ID",
          "value": ""
        },
        {
          "name": "DATABASE_URL",
          "value": "__SET_IN_ECS__"
        },
        {
          "name": "MSG91_TEMPLATE_ID",
          "value": ""
        },
        {
          "name": "JWT_SECRET",
          "value": "__SET_IN_ECS__"
        },
        {
          "name": "SMS_PROVIDER",
          "value": "aws-sns"
        },
        {
          "name": "REDIS_ENABLED",
          "value": "true"
        },
        {
          "name": "AWS_SNS_REGION",
          "value": "ap-south-1"
        },
        {
          "name": "NODE_ENV",
          "value": "production"
        },
        {
          "name": "REDIS_URL",
          "value": "__SET_IN_ECS__"
        },
        {
          "name": "AWS_LOCATION_PLACE_INDEX",
          "value": "weelo-places"
        },
        {
          "name": "GOOGLE_MAPS_API_KEY",
          "value": "__SET_IN_ECS__"
        }
      ],
      "environmentFiles": [],
      "mountPoints": [],
      "volumesFrom": [],
      "ulimits": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "weelobackendtask",
          "awslogs-create-group": "true",
          "awslogs-region": "ap-south-1",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      },
      "systemControls": []
    }
  ],
  "family": "weelobackendtask",
  "taskRoleArn": "arn:aws:iam::318774499084:role/weelo-backend-task-role",
  "executionRoleArn": "arn:aws:iam::318774499084:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024",
  "enableFaultInjection": false
}
